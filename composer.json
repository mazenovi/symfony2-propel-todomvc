{
    "name": "mazenovi/symfony2-propel-todomvc",
    "description": "Symfony2 + Propel variation around todomvc theme (see http://addyosmani.github.com/todomvc/)",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "minimum-stability": "dev",
    "repositories": {
        "TypehintableBehavior": {
            "type": "package",
            "package": {
                "name": "TypehintableBehavior",
                "version": "master",
                "source": {
                    "url": "git://github.com/willdurand/TypehintableBehavior.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        "twitter/bootstrap": {
            "type": "package",
            "package": {
                "version": "master", 
                "name": "twitter/bootstrap",
                "source": {
                    "url": "https://github.com/twitter/bootstrap.git",
                    "type": "git",
                "reference": "master"
                },
                "dist": {
                    "url": "https://github.com/twitter/bootstrap/zipball/master",
                    "type": "zip"
                }
            }
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.x-dev",
        "twig/extensions": "dev-master",
        "symfony/assetic-bundle": "dev-master",
        "symfony/swiftmailer-bundle": "dev-master",
        "symfony/monolog-bundle": "dev-master",
        "sensio/distribution-bundle": "*",
        "sensio/framework-extra-bundle": "*",
        "sensio/generator-bundle": "*",
        "jms/security-extra-bundle": "1.1.*",
        "jms/di-extra-bundle": "1.0.*",
        "propel/propel1": "1.6.x-dev",
        "propel/propel-bundle": "1.1.x-dev",
        "TypehintableBehavior": "master",
        "friendsofsymfony/jsrouting-bundle": "dev-master",
        "friendsofsymfony/rest-bundle": "dev-master",
        "friendsofsymfony/user-bundle": "dev-master",
        "hwi/oauth-bundle": "*",
        "jms/metadata": "dev-master as 1.1.2-dev",
        "manymules/backbonejs-bundle": "*",
        "manymules/jquery-bundle": ">1.7",
        "manymules/json2js-bundle": "*",
        "manymules/requirejs-bundle": "*",
        "manymules/underscorejs-bundle": "*",
        "twitter/bootstrap": "master",
        "mopa/bootstrap-bundle": "dev-master",
        "manymules/fontawesome-bundle": "*",
        "knplabs/knp-menu-bundle": "dev-master",
        "nelmio/api-doc-bundle": "dev-master"
    },
    "require-dev": {
        "behat/behat": "2.4.*@stable",
        "guzzle/guzzle": "dev-master",
        "behat/symfony2-extension": "*",
        "behat/mink":  "1.4.*@stable",
        "behat/mink-extension": "*",      
        "behat/mink-browserkit-driver":  "*",                
        "behat/mink-sahi-driver":  "*"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap",
            "Mazenovi\\TodoMVCBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap",
            "Mazenovi\\TodoMVCBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
